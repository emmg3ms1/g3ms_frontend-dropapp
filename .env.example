# Supabase Configuration
# Get these from your Supabase project dashboard
# Make sure to enable Google and Apple OAuth providers in Supabase Auth settings
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google OAuth Configuration (for Supabase)
# Configure these in your Supabase project settings under Authentication > Providers > Google
# GOOGLE_CLIENT_ID=your_google_client_id_here
# GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Apple OAuth Configuration (for Supabase) 
# Configure these in your Supabase project settings under Authentication > Providers > Apple
# APPLE_CLIENT_ID=your_apple_client_id_here
# APPLE_CLIENT_SECRET=your_apple_client_secret_here

# Optional: Enable analytics
VITE_ENABLE_ANALYTICS=false

# Optional: Allow mock auth in development
VITE_ALLOW_MOCK_AUTH=false
